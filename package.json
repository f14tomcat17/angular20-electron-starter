{
  "name": "angular20-electron-starter",
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "ng serve",
    "electron:compile": "tsc -p electron && npx copyfiles -u 1 electron/assets/* dist-electron",
    "dev": "concurrently \"ng serve\" \"wait-on http://localhost:4200 && npm run electron:compile && electron .\"",
    "build": "ng build && npm run electron:compile",
    "dist": "npm run build && electron-builder --config electron-builder.json --win"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f14tomcat17/angular20-electron-starter.git"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "electron-updater": "^6.6.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.14",
    "@types/semver": "^7.7.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.3"
  }
}
